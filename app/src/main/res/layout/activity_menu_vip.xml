<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/copia2"
    android:theme="@android:style/ThemeOverlay.Material.Dark">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarMenuVip"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:background="@color/negro" />

    <ImageView
       android:layout_marginTop="20dp"
        android:id="@+id/botonSos"
        android:layout_marginLeft="40dp"
       android:layout_below="@id/toolbarMenuVip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:src="@drawable/sos"

        />

    <ImageView

        android:id="@+id/botonVerCamara"
        android:layout_width="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_below="@id/botonSos"
        android:src="@drawable/ver1" />




    <ImageView

        android:id="@+id/botonVerFotos"
        android:layout_below="@id/botonVerCamara"
        android:layout_width="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/ver"

        />

    <ImageView

        android:id="@+id/botonListarIncidencias"
        android:layout_below="@id/botonVerFotos"
        android:layout_width="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@drawable/notificacion"

        />


</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    android:background="@drawable/imagen"
    tools:context=".MenuUsuarioPelon">




    <ImageView

        android:id="@+id/imagenlistarXML"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
      android:layout_centerHorizontal="true"

        android:src="@drawable/b3"

        />
    <ImageView
        android:layout_marginTop="20dp"
        android:id="@+id/imagenver"
        android:layout_below="@id/imagenlistarXML"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"

        android:src="@drawable/b7"

        />
    <ImageView
        android:layout_marginTop="20dp"
        android:id="@+id/imagenactualizar"
        android:layout_below="@id/imagenver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"

        android:src="@drawable/b6"

        />




</RelativeLayout>
